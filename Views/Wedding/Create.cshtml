@{
    ViewData["Title"] = "Dashboard";
}
@model WeddingPlanner.Models.ViewWedding

<h1>Plan Your Wedding</h1>
<form method="post" asp-controller="Wedding" asp-action="Create">
    <label asp-for="WedderOne"></label>
    <input asp-for="WedderOne">
    <span asp-validation-for="WedderOne"></span><br>

    <label asp-for="WedderTwo"></label>
    <input asp-for="WedderTwo">
    <span asp-validation-for="WedderTwo"></span><br>

    <label asp-for="Date"></label>
    <input asp-for="Date">
    <span asp-validation-for="Date"></span><br>

    <label asp-for="Address"></label>
    <input asp-for="Address">
    <span asp-validation-for="Address"></span><br>

    <input type="submit" value="Create">
</form>    
